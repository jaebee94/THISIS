<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.curation.dao.TagDAO">

	<!-- 회원정보 전체 조회 -->
	<select id="selectTag" resultType="Tag">
		select *
		from tag
		where tagid=#{tagid}
	</select>
	
	<insert id="createTag">
		insert into tag (tagid, tagname)
		values(#{tagid}, #{tagname})
	</insert>

	<delete id="deleteTag">
		delete from tag
		where tagid = #{tagid} 
	</delete>
</mapper>